<p id="notice"><%= notice %></p>

<h1>Transcript Configs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Config</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transcript_configs.each do |transcript_config| %>
      <tr>
        <td><%= transcript_config.name %></td>
        <td><%= transcript_config.config %></td>
        <td><%= link_to 'Show', transcript_config %></td>
        <td><%= link_to 'Edit', edit_transcript_config_path(transcript_config) %></td>
        <td><%= link_to 'Destroy', transcript_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transcript Config', new_transcript_config_path %>
